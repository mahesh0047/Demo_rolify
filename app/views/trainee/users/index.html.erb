<p id="notice"><%= notice %></p>


<div class="page-header">
  <h1>Users</h1>
</div>

<table class="table table-dark">
  <thead class="bg-secondary text-white">
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Phone no</th>
      <th>State</th>
      <th>City</th>
      <th>company</th>
      <th>Role</th>
      <th colspan="3" ></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name.titleize %></td>
        <td><%= user.email %></td>
        <td><%= user.phone_no %></td>
        <td><%= user.state.name.titleize %></td>
        <td><%= user.city.name.titleize %></td>
        <td><%= user.company&.name.titleize %></td>
        <td><%= user.roles.first&.name&.titleize %></td>
        <td><%= link_to 'Show',  trainee_user_path(user), class: 'btn btn-success text-white' %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>




