  </tr>

  <tr>
    <td><strong>City:</strong></td>
    <td><%= @user.city.name.titleize %></td>
  </tr>

  <tr>
    <td><strong>Company:</strong></td>
    <td><%= @user.company.name %></td>
  </tr>
</table>

