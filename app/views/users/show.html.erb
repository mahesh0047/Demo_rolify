<p id="notice"><%= notice %></p>


<table class="table table-dark" style="width: auto;">
  <tr>
    <td><strong>Name:</strong></td>
    <td><%= @user.name.titleize %></td>
  </tr>

  <tr>
    <td><strong>Email:</strong></td>
    <td><%= @user.email %></td>
  </tr>

  <tr>
    <td><strong>Phone no:</strong></td>
    <td><%= @user.phone_no %></td>
  </tr>

  <tr>
    <td><strong>State:</strong></td>
